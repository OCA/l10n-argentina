<?xml version="1.0" encoding="utf-8"?>
<odoo>


        <record id="comp_consultar_wizard_view" model="ir.ui.view">
            <field name="name">Wizard Consultar Comprobante</field>
            <field name="model">wizard.comp.consultar</field>
            <field name="arch" type="xml">
                <form string="Crear Ruta Entrega">
                        <sheet>
                                <group>
                                        <field name="journal_id" readonly="1" />
                                        <field name="doc_type" readonly="1" />
                                        <field name="doc_nro" />
                                </group>
                    </sheet>
                    <footer>
                        <button string="Confirmar" name="btn_confirm" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>



